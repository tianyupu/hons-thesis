@ARTICLE{Clark2007,
  author = {Clark, David E and Lucas, F L and Ryan, Louise M},
  title = {Predicting hospital mortality, length of stay, and transfer to long-term
	care for injured patients.},
  journal = {Journal of Trauma: Injury Infection \& Critical Care},
  year = {2007},
  volume = {62},
  pages = {592--600},
  number = {3},
  month = {March},
  abstract = {Background: Using hospital length of stay (LOS) to measure trauma
	care efficiency is complicated by short LOS resulting from early
	mortality or transfer to long-term care (LTC).
	
	
	Methods: Records from the 1999 to 2003 National Trauma Data Bank were
	used to create a multistate model divided into four time periods,
	each with constant rates of death, discharge home, and LTC transfer.
	Estimated hospital mortality and LOS for patient subgroups were calculated
	from this model, and time-varying covariate effects were estimated.
	
	
	Results: A total of 369,829 cases with adequate data were available.
	Early mortality was increased most by hypotension or coma, and also
	by anatomic injury severity or penetrating mechanism, but these effects
	diminished with time; age remained a strong predictor of mortality
	at any time but sex was insignificant. Rates of discharge home decreased
